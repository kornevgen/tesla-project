VAR INT fs;
VAR INT ft;
VAR INT cond;

ASSERT 0 <= cond AND cond < 16;

ASSERT SNaN_single( fs ) OR SNaN_single( ft ) 
	OR QNaN_single( fs ) OR QNaN_single( ft ) ;

SITUATION InvalidOperation WHEN 
	SNaN_single( fs ) OR SNaN_single( ft ) 
OR cond[3] = 1 AND ( SNaN_single( fs ) OR QNaN_single( ft ) ) .
